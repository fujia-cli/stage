{
  "name": "@fujia/stage-models",
  "description": "The models for Stage CLI",
  "version": "0.2.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup",
    "check-exports": "attw --pack ."
  },
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@fujia/typescript-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/semver": "^7.5.8"
  },
  "homepage": "https://github.com/fujia-cli/stage",
  "bugs": {
    "url": "https://github.com/fujia-cli/stage/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fujia-cli/stage",
    "directory": "https://github.com/fujia-cli/stage"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "dependencies": {
    "@fujia/check-path": "^1.2.0",
    "@fujia/hammer": "^1.0.0",
    "@fujia/stage-share": "^0.1.0",
    "@fujia/user-home": "^1.1.1",
    "@inquirer/prompts": "^7.2.3",
    "axios": "^1.7.9",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.3.2",
    "npminstall": "^7.12.0",
    "pkg-dir": "^8.0.0",
    "semver": "^7.6.3",
    "simple-git": "^3.27.0",
    "terminal-link": "^3.0.0"
  }
}
