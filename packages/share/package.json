{
  "name": "@fujia/stage-share",
  "description": "The share tools for Stage CLI",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup",
    "check-exports": "attw --pack ."
  },
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@fujia/typescript-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/npmlog": "^7.0.0",
    "@types/semver": "^7.5.8",
    "@types/url-join": "^4.0.3"
  },
  "homepage": "https://github.com/fujia-cli/stage",
  "bugs": {
    "url": "https://github.com/fujia-cli/stage/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fujia-cli/stage",
    "directory": "https://github.com/fujia-cli/stage"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "node.js",
    "cli",
    "stage",
    "react",
    "npm",
    "tools"
  ],
  "dependencies": {
    "@fujia/check-path": "^1.2.0",
    "@fujia/cli-log": "^1.5.0",
    "@fujia/cli-utils": "^1.5.0",
    "axios": "^1.7.9",
    "commander": "^13.0.0",
    "fs-extra": "^11.3.0",
    "npmlog": "^7.0.1",
    "ora": "^8.1.1",
    "semver": "^7.6.3",
    "url-join": "^5.0.0"
  }
}
